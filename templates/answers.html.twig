{#
/**
 * @file answers.html.twig
 * Default theme implementation to present Answers data.
 *
 * This template is used when viewing Answers pages.
 *
 *
 * Available variables:
 * - content: A list of content items. Use 'content' to print all content, or
 * - attributes: HTML attributes for the container element.
 *
 * @see template_preprocess_answers()
 *
 * @ingroup themeable
 */
#}
<div{{ attributes.addClass('answers') }}>
  {% if content %}
    {{- content -}}
  {% endif %}
</div>
